# openrouter configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=qwen/qwen3-max
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_REFERER=
OPENROUTER_TITLE=hyperliquid-agent

# trading backend: perpsim (paper) or hyperliquid (live testnet)
TRADING_BACKEND=perpsim

# hyperliquid testnet (only needed if TRADING_BACKEND=hyperliquid)
HL_API_KEY=
HL_API_SECRET=
HL_ACCOUNT=
HL_BASE_URL=https://api.hyperliquid-testnet.xyz

# database
DATABASE_URL=postgresql://user:password@localhost:5432/hyperliquid_agent

# agent settings
CYCLE_INTERVAL_SECONDS=600
MIN_NOTIONAL=5.0
MAX_LEVERAGE=20.0
DRY_RUN=false

# perpsim config (paper trading simulator)
SIM_IM=0.05
SIM_MM=0.03
SIM_MAX_LEVERAGE=20.0
SIM_SLIPPAGE_BPS=1.0
SIM_FEE_BPS=2.0
SIM_LIQ_PENALTY_BPS=5.0
SIM_FUNDING_MODE=A
SIM_SYMBOLS=BTC-USD,ETH-USD
SIM_TICK_SIZE=0.5

# data feed
DATA_FEED=coinbase
COINBASE_WS_URL=wss://ws-feed.exchange.coinbase.com

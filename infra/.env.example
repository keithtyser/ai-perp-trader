# openrouter
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=qwen/qwen3-max

# trading backend: perpsim (paper) or hyperliquid (live testnet)
TRADING_BACKEND=perpsim

# hyperliquid testnet (only needed if TRADING_BACKEND=hyperliquid)
